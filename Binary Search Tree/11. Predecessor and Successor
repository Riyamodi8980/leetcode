def findPreSuc(root, pre, suc, key):
    def inorder(node):
    
        if node==None:
            return 
        
        
        inorder(node.left)
        
        if node.key<key:
            pre[0]=node
        if node.key>key and suc[0]==None:
            suc[0]=node
            return
        
        inorder(node.right)
        
    inorder(root)
